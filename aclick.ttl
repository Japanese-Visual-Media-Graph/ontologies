@prefix : <http://mediagraph.link/aclick/ont/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ns1: <http://mediagraph.link/jvmg/ont/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rfs: <http://mediagraph.link/aclick/ont/rfs:> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix aclick: <http://mediagraph.link/aclick/ont/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://mediagraph.link/aclick/ont/> .

<http://mediagraph.link/aclick/ont/> rdf:type owl:Ontology ;
                                      dc:creator "Japanese Visual Media Graph" ;
                                      dc:description "A metadata vocabulary based on AnimeClick.it, with property names based primarily on the original data."@en ;
                                      dc:title "AnimeClick.it Ontology"@en ;
                                      dcterms:modified "2021-05-05" ;
                                      vann:preferredNamespacePrefix "aclick" ;
                                      vann:preferredNamespaceUri <http://mediagraph.link/aclick/ont> ;
                                      rdfs:comment "Some modifications, such as the defining of class & sublcass relationships have been made from the original AnimeClick data model."@en ;
                                      rdfs:label "AnimeClick Ontology"@en ;
                                      owl:versionInfo "1.3"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://mediagraph.link/aclick/ont/rfs:type
rfs:type rdf:type owl:AnnotationProperty .


###  http://mediagraph.link/jvmg/ont/sourceGraph
ns1:sourceGraph rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://mediagraph.link/aclick/ont/author
aclick:author rdf:type owl:ObjectProperty ;
              rdfs:domain aclick:Staff ;
              rdfs:range aclick:Author ;
              ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
              rdfs:label "Author"@en ;
              skos:definition "Author or primary creator of a creative work"@en .


###  http://mediagraph.link/aclick/ont/category
aclick:category rdf:type owl:ObjectProperty ;
                rdfs:domain aclick:Anime ,
                            aclick:Manga ;
                rdfs:range aclick:Category ;
                ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
                rdfs:label "Category"@en ;
                skos:definition "Indicates an anime or manga category"@en .


###  http://mediagraph.link/aclick/ont/character
aclick:character rdf:type owl:ObjectProperty ;
                 rdfs:domain aclick:CreativeWork ;
                 rdfs:range aclick:Character ;
                 ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
                 rdfs:label "Character"@en ;
                 skos:definition "Indicates characters who appears in an anime"@en .


###  http://mediagraph.link/aclick/ont/episodes
aclick:episodes rdf:type owl:ObjectProperty ;
                rdfs:domain aclick:CreativeWork ;
                rdfs:range aclick:Episode ;
                ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
                rdfs:label "Episodes"@en ;
                skos:definition "Lists individual anime episodes"@en .


###  http://mediagraph.link/aclick/ont/genre
aclick:genre rdf:type owl:ObjectProperty ;
             rdfs:domain aclick:Anime ,
                         aclick:Manga ;
             rdfs:range aclick:Genre ;
             ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
             rdfs:label "Genre"@en ;
             skos:definition "Indicates a genre of a creative work"@en .


###  http://mediagraph.link/aclick/ont/related
aclick:related rdf:type owl:ObjectProperty ;
               rdfs:domain aclick:CreativeWork ;
               rdfs:range aclick:CreativeWork ;
               ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
               rdfs:label "Related"@en ;
               skos:definition "Indicates a related work."@en .


###  http://mediagraph.link/aclick/ont/tag
aclick:tag rdf:type owl:ObjectProperty ;
           rdfs:domain aclick:Anime ,
                       aclick:Manga ;
           rdfs:range aclick:Tag ;
           ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
           rdfs:label "Tag"@en ;
           skos:definition "A descriptive content tag for a creative work."@en .


###  http://mediagraph.link/aclick/ont/type
aclick:type rdf:type owl:ObjectProperty ;
            rdfs:domain aclick:Anime ,
                        aclick:Author ,
                        aclick:Manga ,
                        aclick:Volume ;
            rdfs:range aclick:Type ;
            ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
            rdfs:label "Type"@en ;
            skos:definition "Indicates a media type or author type"@en .


###  http://mediagraph.link/aclick/ont/voices
aclick:voices rdf:type owl:ObjectProperty ;
              rdfs:domain aclick:VoiceActor ;
              rdfs:range aclick:Character ;
              ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
              rdfs:label "Voices"@en ;
              skos:definition "Indicates a voice actor of a creative work"@en .


###  http://mediagraph.link/aclick/ont/volumes
aclick:volumes rdf:type owl:ObjectProperty ;
               rdfs:domain aclick:Manga ;
               rdfs:range aclick:Volume ;
               ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
               rdfs:label "volumes"@en ;
               skos:definition "Lists individual manga volumes"@en .


###  http://mediagraph.link/aclick/ont/work
aclick:work rdf:type owl:ObjectProperty ;
            rdfs:domain aclick:Staff ;
            rdfs:range aclick:CreativeWork ;
            ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
            rdfs:label "Work"@en ;
            skos:definition "Indicates the staff of a creative work (e.g. manga, anime)"@en .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:subPropertyOf aclick:episodes .


#################################################################
#    Data properties
#################################################################

###  http://mediagraph.link/aclick/ont/aclickLink
aclick:aclickLink rdf:type owl:DatatypeProperty ;
                  rdfs:domain aclick:Anime ,
                              aclick:Author ,
                              aclick:Episode ;
                  rdfs:range xsd:anyURI ;
                  ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
                  rdfs:label "Aclick Link"@en ;
                  skos:definition "Link to aclick website"@en .


###  http://mediagraph.link/aclick/ont/blog
aclick:blog rdf:type owl:DatatypeProperty ;
            rdfs:domain aclick:Author ;
            rdfs:range rdfs:Literal ,
                       xsd:anyURI ;
            ns1:sourceGraph <http://mediagraph.link/graph/aclick> ;
            rdfs:label "Blog"@en ;
            skos:definition "Blog URL of a resource."@en .


###  http://mediagraph.link/aclick/ont/dateOfBirth
aclick:dateOfBirth rdf:type owl:DatatypeProperty ;
