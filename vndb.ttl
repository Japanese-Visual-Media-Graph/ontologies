@prefix : <http://mediagraph.link/vndb/ont/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix jvmg: <http://mediagraph.link/jvmg/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix vndb: <http://mediagraph.link/vndb/ont/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://mediagraph.link/vndb/ont/> .

<http://mediagraph.link/vndb/ont/> rdf:type owl:Ontology ;
                                    dc:creator "Japanese Visual Media Graph" ;
                                    dc:title "VNDB Ontology"@en ;
                                    dcterms:description "A metadata vocabulary based on the Visual Novel Database with property names based primarily on the original VNDB exported data."@en ;
                                    dcterms:modified "2023-11-23" ;
                                    vann:preferredNamespacePrefix "vndb" ;
                                    vann:preferredNamespaceUri <http://mediagraph.link/vndb/ont/> ;
                                    rdfs:comment "Information regarding some guidelines used to create this ontology can be found at various Editing Guidelines sections which can be found gathered at https://vndb.org/d6 ."@en ,
                                                 "Some modifications, such as the defining of class & sublcass relationships have been made from the original VNDB data model."@en ;
                                    rdfs:label "VNDB Ontology"@en ;
                                    owl:versionInfo "1.2"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://mediagraph.link/jvmg/ont/sourceGraph
jvmg:sourceGraph rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#resources
rdfs:resources rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://mediagraph.link/vndb/ont/anime
vndb:anime rdf:type owl:ObjectProperty ;
           rdfs:domain vndb:VisualNovel ;
           rdfs:range vndb:Anime ;
           jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
           rdfs:label "Anime"@en ;
           skos:definition "Anime release of a visual novel"@en .


###  http://mediagraph.link/vndb/ont/appearsIn
vndb:appearsIn rdf:type owl:ObjectProperty ;
               owl:inverseOf vndb:appearsIn ;
               rdfs:domain vndb:Character ;
               rdfs:range vndb:VisualNovel ;
               jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
               rdfs:label "Appears in"@en ;
               skos:definition "Visual novel featuring the character in an appearance role."@en .


###  http://mediagraph.link/vndb/ont/character
vndb:character rdf:type owl:ObjectProperty ;
               owl:inverseOf vndb:character ;
               rdfs:domain vndb:Role ;
               rdfs:range vndb:Character ;
               jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
               rdfs:label "Character"@en ;
               skos:definition "The character of a certain visual novel."@en .


###  http://mediagraph.link/vndb/ont/developed
vndb:developed rdf:type owl:ObjectProperty ;
               rdfs:domain vndb:Producer ;
               rdfs:range vndb:VisualNovelRelease ;
               rdfs:label "Developed"@en ;
               skos:definition "Indicates which producer developed a visual novel."@en .


###  http://mediagraph.link/vndb/ont/group
vndb:group rdf:type owl:ObjectProperty ;
           rdfs:domain vndb:Trait ;
           rdfs:range vndb:Trait ;
           jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
           rdfs:label "Group"@en ;
           skos:definition "Indicates groups of traits"@en .


###  http://mediagraph.link/vndb/ont/main
vndb:main rdf:type owl:ObjectProperty ;
          owl:inverseOf vndb:main ;
          rdfs:domain vndb:Character ;
          rdfs:range vndb:Character ;
          jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
          rdfs:label "Other instance"@en ;
          skos:definition "Another instance of the character."@en .


###  http://mediagraph.link/vndb/ont/mainIn
vndb:mainIn rdf:type owl:ObjectProperty ;
            owl:inverseOf vndb:mainIn ;
            rdfs:domain vndb:Character ;
            rdfs:range vndb:VisualNovel ;
            jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
            rdfs:label "Main role"@en ;
            skos:definition "Visual novel featuring the character in a main role."@en .


###  http://mediagraph.link/vndb/ont/primaryIn
vndb:primaryIn rdf:type owl:ObjectProperty ;
               owl:inverseOf vndb:primaryIn ;
               rdfs:domain vndb:Character ;
               rdfs:range vndb:VisualNovel ;
               jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
               rdfs:label "Primary role"@en ;
               skos:definition "Visual novel featuring the character in a primary or protagonist role."@en .


###  http://mediagraph.link/vndb/ont/producerImp
vndb:producerImp rdf:type owl:ObjectProperty ;
                 owl:inverseOf vndb:producerIpa ;
                 rdfs:domain vndb:Producer ;
                 rdfs:range vndb:Producer ;
                 jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                 rdfs:label "Imprint"@en ;
                 skos:definition "Identifies an imprint of a producer."@en .


###  http://mediagraph.link/vndb/ont/producerIpa
vndb:producerIpa rdf:type owl:ObjectProperty ;
                 rdfs:domain vndb:Producer ;
                 rdfs:range vndb:Producer ;
                 jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                 rdfs:label "Parent brand"@en ;
                 skos:definition "Identifies the parent brand of a producer."@en .


###  http://mediagraph.link/vndb/ont/producerNew
vndb:producerNew rdf:type owl:ObjectProperty ;
                 owl:inverseOf vndb:producerOld ;
                 rdfs:domain vndb:Producer ;
                 rdfs:range vndb:Producer ;
                 jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                 rdfs:label "Succeeded"@en ;
                 skos:definition "Indicates a succession relationship between producers."@en .


###  http://mediagraph.link/vndb/ont/producerOld
vndb:producerOld rdf:type owl:ObjectProperty ;
                 rdfs:domain vndb:Producer ;
                 rdfs:range vndb:Producer ;
                 jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                 rdfs:label "Formerly"@en ;
                 skos:definition "Indicates the name that the producer was previously known as."@en .


###  http://mediagraph.link/vndb/ont/producerOri
vndb:producerOri rdf:type owl:ObjectProperty ;
                 owl:inverseOf vndb:producerSpa ;
                 rdfs:domain vndb:Producer ;
                 rdfs:range vndb:Producer ;
                 jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                 rdfs:label "Originated"@en ;
                 skos:definition "Identifies another producer from which the select producer originated from."@en .


###  http://mediagraph.link/vndb/ont/producerPar
vndb:producerPar rdf:type owl:ObjectProperty ;
                 owl:inverseOf vndb:producerSub ;
                 rdfs:domain vndb:Producer ;
                 rdfs:range vndb:Producer ;
                 jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                 rdfs:label "Parent producer"@en ;
                 skos:definition "Identifies the parent producer of a producer."@en .


###  http://mediagraph.link/vndb/ont/producerSpa
vndb:producerSpa rdf:type owl:ObjectProperty ;
                 rdfs:domain vndb:Producer ;
                 rdfs:range vndb:Producer ;
                 jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                 rdfs:label "Spawned"@en ;
                 skos:definition "Identifies another producer which spawned the select producer."@en .


###  http://mediagraph.link/vndb/ont/producerSub
vndb:producerSub rdf:type owl:ObjectProperty ;
                 rdfs:domain vndb:Producer ;
                 rdfs:range vndb:Producer ;
                 jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                 rdfs:label "Subsidiary"@en ;
                 skos:definition "Indicates a subsidiary of the select producer."@en .


###  http://mediagraph.link/vndb/ont/published
vndb:published rdf:type owl:ObjectProperty ;
               rdfs:domain vndb:Producer ;
               rdfs:range vndb:VisualNovelRelease ;
               rdfs:label "Published"@en ;
               skos:definition "Indicates which producer published a visual novel."@en .


###  http://mediagraph.link/vndb/ont/related
vndb:related rdf:type owl:ObjectProperty ;
             owl:inverseOf vndb:related ,
                           vndb:sideIn ;
             rdfs:domain vndb:VisualNovel ;
             rdfs:range vndb:VisualNovel ;
             jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
             rdfs:label "Related"@en ;
             skos:definition "Indicates a relationship between different visual novels."@en .


###  http://mediagraph.link/vndb/ont/release
vndb:release rdf:type owl:ObjectProperty ;
             rdfs:domain vndb:VisualNovel ;
             rdfs:range vndb:VisualNovelRelease ;
             jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
             rdfs:label "Release"@en ;
             skos:definition "A single release of a visual novel."@en .


###  http://mediagraph.link/vndb/ont/released
vndb:released rdf:type owl:ObjectProperty ;
              rdfs:domain vndb:Producer ;
              rdfs:range vndb:VisualNovel ;
              jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
              rdfs:label "Released"@en ;
              skos:definition "Indicates the release of a visual novel by a producer"@en .


###  http://mediagraph.link/vndb/ont/sideIn
vndb:sideIn rdf:type owl:ObjectProperty ;
            rdfs:domain vndb:Character ;
            rdfs:range vndb:VisualNovel ;
            jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
            rdfs:label "Side role"@en ;
            skos:definition "Visual novel featuring the character in a side role."@en .


###  http://mediagraph.link/vndb/ont/staff
vndb:staff rdf:type owl:ObjectProperty ;
           owl:inverseOf vndb:staff ;
           rdfs:domain vndb:Role ;
           rdfs:range vndb:Staff ;
           jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
           rdfs:label "Staff"@en ;
           skos:definition "A resource which the staff contributed to."@en .


###  http://mediagraph.link/vndb/ont/staffAlias
vndb:staffAlias rdf:type owl:ObjectProperty ;
                owl:inverseOf vndb:staffAlias ;
                rdfs:domain vndb:Role ,
                            vndb:Staff ;
                rdfs:range vndb:Staff ;
                jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                rdfs:label "Staff alias"@en ;
                skos:definition "An alias name or title of a given resource."@en .


###  http://mediagraph.link/vndb/ont/tag
vndb:tag rdf:type owl:ObjectProperty ;
         owl:inverseOf vndb:tag ;
         rdfs:domain vndb:VisualNovel ;
         rdfs:range vndb:Tag ;
         jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
         rdfs:label "Tag"@en ;
         skos:definition "A descriptive content tag for a visual novel."@en .


###  http://mediagraph.link/vndb/ont/tagParent
vndb:tagParent rdf:type owl:ObjectProperty ;
               owl:inverseOf vndb:tagParent ;
               rdfs:domain vndb:Tag ;
               rdfs:range vndb:Tag ;
               jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
               rdfs:label "Tag parent"@en ;
               skos:definition "The hierarchical parent tag."@en .


###  http://mediagraph.link/vndb/ont/trait
vndb:trait rdf:type owl:ObjectProperty ;
           owl:inverseOf vndb:trait ;
           rdfs:domain vndb:Character ;
           rdfs:range vndb:Trait ;
           jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
           rdfs:label "Trait"@en ;
           skos:definition "A descriptive content trait for a visual novel character."@en .


###  http://mediagraph.link/vndb/ont/traitParent
vndb:traitParent rdf:type owl:ObjectProperty ;
                 owl:inverseOf vndb:traitParent ;
                 rdfs:domain vndb:Trait ;
                 rdfs:range vndb:Trait ;
                 jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                 rdfs:label "Trait parent"@en ;
                 skos:definition "The hierarchical parent trait."@en .


###  http://mediagraph.link/vndb/ont/visualNovelLength
vndb:visualNovelLength rdf:type owl:ObjectProperty ;
                       rdfs:domain vndb:VisualNovel ;
                       rdfs:range vndb:VisualNovelLength ;
                       jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                       rdfs:label "Visual novel length"@en ;
                       skos:definition "Indicates the lenght of a visual novel"@en .


###  http://mediagraph.link/vndb/ont/vn
vndb:vn rdf:type owl:ObjectProperty ;
        owl:inverseOf vndb:vn ;
        rdfs:domain vndb:Role ;
        rdfs:range vndb:VisualNovel ;
        jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
        rdfs:label "Visual novel"@en ;
        skos:definition "Character appears in a certain visual novel"@en .


#################################################################
#    Data properties
#################################################################

###  http://mediagraph.link/jvmg/ont/count
jvmg:count rdf:type owl:DatatypeProperty ;
           rdfs:domain owl:Thing ;
           rdfs:range rdfs:resources ,
                      xsd:integer ;
           rdfs:label "Count"@en ;
           skos:definition "Indicates how often a property exists"@en .


###  http://mediagraph.link/jvmg/ont/order
jvmg:order rdf:type owl:DatatypeProperty ;
           rdfs:domain owl:Thing ;
           rdfs:range rdfs:resources ,
                      xsd:integer ;
           rdfs:label "Order"@en ;
           skos:definition "order of display in the search overview"@en .


###  http://mediagraph.link/vndb/ont/age
vndb:age rdf:type owl:DatatypeProperty ;
         rdfs:domain vndb:Character ;
         rdfs:range xsd:integer ;
         jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
         rdfs:label "Age"@en ;
         skos:definition "Age of a character"@en .


###  http://mediagraph.link/vndb/ont/aid
vndb:aid rdf:type owl:DatatypeProperty ;
         rdfs:range rdfs:Literal ;
         jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
         rdfs:label "internal AID"@en .


###  http://mediagraph.link/vndb/ont/alias
vndb:alias rdf:type owl:DatatypeProperty ;
           rdfs:domain vndb:Character ,
                       vndb:Producer ,
                       vndb:Staff ,
                       vndb:Tag ,
                       vndb:Trait ,
                       vndb:VisualNovel ;
           rdfs:range rdfs:Literal ;
           jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
           rdfs:label "Alias"@en ;
           skos:definition "An alias name or title of a given resource."@en .


###  http://mediagraph.link/vndb/ont/aliasOriginal
vndb:aliasOriginal rdf:type owl:DatatypeProperty ;
                   rdfs:domain vndb:Staff ;
                   rdfs:range rdfs:Literal ;
                   jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                   rdfs:label "Alias original"@en ;
                   skos:definition "Alias name or title in the original language."@en .


###  http://mediagraph.link/vndb/ont/ani_ero
vndb:ani_ero rdf:type owl:DatatypeProperty ;
             jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
             rdfs:label "Ani ero"@en .


###  http://mediagraph.link/vndb/ont/ani_story
vndb:ani_story rdf:type owl:DatatypeProperty ;
               jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
               rdfs:label "Ani story"@en .


###  http://mediagraph.link/vndb/ont/applicable
vndb:applicable rdf:type owl:DatatypeProperty ;
                rdfs:domain vndb:Tag ,
                            vndb:Trait ;
                rdfs:range xsd:boolean ;
                jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                rdfs:label "Applicable"@en ;
                skos:definition "Indicates whether a tag or trait is applicable"@en .


###  http://mediagraph.link/vndb/ont/bDay
vndb:bDay rdf:type owl:DatatypeProperty ;
          rdfs:domain vndb:Character ;
          rdfs:range xsd:integer ;
          jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
          rdfs:label "Birth day"@en ;
          skos:definition "Birth day of a character."@en .


###  http://mediagraph.link/vndb/ont/bMonth
vndb:bMonth rdf:type owl:DatatypeProperty ;
            rdfs:domain vndb:Character ;
            rdfs:range xsd:integer ;
            jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
            rdfs:label "Birth month"@en ;
            skos:definition "Birth month of a character."@en .


###  http://mediagraph.link/vndb/ont/bloodt
vndb:bloodt rdf:type owl:DatatypeProperty ;
            rdfs:domain vndb:Character ;
            rdfs:range rdfs:Literal ;
            jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
            rdfs:label "Blood type"@en ;
            skos:definition "Blood type of a character."@en .


###  http://mediagraph.link/vndb/ont/cat
vndb:cat rdf:type owl:DatatypeProperty ;
         rdfs:domain vndb:Tag ;
         rdfs:range rdfs:Literal ;
         jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
         rdfs:label "Category"@en ;
         skos:definition "Indicates the category of a visual novel"@en .


###  http://mediagraph.link/vndb/ont/catalog
vndb:catalog rdf:type owl:DatatypeProperty ;
             rdfs:range rdfs:Literal ;
             jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
             rdfs:label "Catalog ID"@en ;
             skos:definition "Catalog identifier of a resource."@en .


###  http://mediagraph.link/vndb/ont/cupSize
vndb:cupSize rdf:type owl:DatatypeProperty ;
             rdfs:domain vndb:Character ;
             rdfs:range rdfs:Literal ;
             jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
             rdfs:label "Cup size"@en ;
             skos:definition "Cup size of a character"@en .


###  http://mediagraph.link/vndb/ont/defaultspoil
vndb:defaultspoil rdf:type owl:DatatypeProperty ;
                  rdfs:domain vndb:Tag ,
                              vndb:Trait ;
                  rdfs:range rdfs:Literal ;
                  jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                  rdfs:label "Default spoil status"@en ;
                  skos:definition "Indicates the default spoiler status for tags and traits"@en .


###  http://mediagraph.link/vndb/ont/desc
vndb:desc rdf:type owl:DatatypeProperty ;
          rdfs:domain vndb:Character ;
          rdfs:range rdfs:Literal ;
          jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
          rdfs:comment "A short description of the character. While it is advised to keep the spoilers to a minimum, you can use the usual formatting codes such as [spoiler] to include spoilers while hiding them by default. When using a description from external sources, check out the general editing guidelines for quoting rules.  [Source https://vndb.org/d12 ]"@en ;
          rdfs:label "description"@en ;
          skos:definition "Description of a given resource."@en .


###  http://mediagraph.link/vndb/ont/developer
vndb:developer rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:boolean ;
               jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
               rdfs:comment "The producer involved in the creation of the game itself, not necessarily of this specific release. Keep in mind that producers that have made modifications to a game but have not made the game itself should NOT be listed as developers.  [Source https://vndb.org/d3 ]"@en ;
               rdfs:label "Developer role"@en .


###  http://mediagraph.link/vndb/ont/doujin
vndb:doujin rdf:type owl:DatatypeProperty ;
            rdfs:domain vndb:VisualNovelRelease ;
            rdfs:range xsd:boolean ;
            jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
            rdfs:label "Doujin"@en ;
            skos:definition "Indicates whether or not a resource was published by a doujin circle, amateur group, or individual, rather than a corporate body."@en .


###  http://mediagraph.link/vndb/ont/engine
vndb:engine rdf:type owl:DatatypeProperty ;
            rdfs:domain vndb:VisualNovelRelease ;
            rdfs:range rdfs:Literal ;
            jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
            rdfs:label "Game engine"@en ;
            skos:definition "Indentifies the game engine used for a release."@en .


###  http://mediagraph.link/vndb/ont/externalLink
vndb:externalLink rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:anyURI ;
                  jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
                  rdfs:label "External link"@en ;
                  skos:definition "A link to a non-VNDB website."@en .


###  http://mediagraph.link/vndb/ont/freeware
vndb:freeware rdf:type owl:DatatypeProperty ;
              rdfs:domain vndb:VisualNovelRelease ;
              rdfs:range xsd:boolean ;
              jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
              rdfs:label "Freeware"@en ;
              skos:definition "Indicates whether or not a resource was released as freeware."@en .


###  http://mediagraph.link/vndb/ont/gender
vndb:gender rdf:type owl:DatatypeProperty ;
            rdfs:domain vndb:Character ,
                        vndb:Staff ;
            rdfs:range rdfs:Literal ;
            jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
            rdfs:label "Gender"@en ;
            skos:definition "The gender of a character or staff member"@en .


###  http://mediagraph.link/vndb/ont/gtin
vndb:gtin rdf:type owl:DatatypeProperty ;
          rdfs:range rdfs:Literal ;
          jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
          rdfs:comment "The GTIN code of the product. Often called \"JAN\" for Japanese releases, \"UPC\" for USA and Canada and \"EAN\" for Europe. The system will automatically detect the type from the code and use the appropriate term on the release page. [Source: https://vndb.org/d3 ]"@en ;
          rdfs:label "GTIN ID"@en ;
          skos:definition "Global Trade Item Number or regional equivalent of a resource."@en .


###  http://mediagraph.link/vndb/ont/height
vndb:height rdf:type owl:DatatypeProperty ;
            rdfs:domain vndb:Character ;
            rdfs:range xsd:integer ;
            jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
            rdfs:label "Height"@en ;
            skos:definition "Character height in cm."@en .


###  http://mediagraph.link/vndb/ont/id
vndb:id rdf:type owl:DatatypeProperty ;
        rdfs:range rdfs:Literal ;
        jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
        rdfs:label "ID"@en ;
        skos:definition "The identifier value of a given resource in VNDB."@en .


###  http://mediagraph.link/vndb/ont/image
vndb:image rdf:type owl:DatatypeProperty ;
           jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
           rdfs:label "image"@en .


###  http://mediagraph.link/vndb/ont/img_nsfw
vndb:img_nsfw rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:boolean ;
              jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
              rdfs:label "image NSFW"@en ;
              skos:definition "Indicates whether or not an image is 'not safe for work'."@en .


###  http://mediagraph.link/vndb/ont/l_denpa
vndb:l_denpa rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf vndb:externalLink ;
             rdfs:range xsd:anyURI ;
             jvmg:sourceGraph <http://mediagraph.link/graph/vndb> ;
