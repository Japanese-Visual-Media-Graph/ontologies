#################################################################
#    Annotation properties
#################################################################

###  http://mediagraph.link/anidb/ont/comment
anidb:comment rdf:type owl:AnnotationProperty ;
              rdfs:comment "A comment taken directly from AniDB providing additional insight on a property. Mainly sourced from the AniDB wiki, found at https://wiki.anidb.net/Content:Titles ."@en ;
              rdfs:label "AniDB Comment"@en .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Data properties
#################################################################

###  http://mediagraph.link/anidb/ont/anidbLink
anidb:anidbLink rdf:type owl:DatatypeProperty ;
                rdfs:comment "A link to the AniDB entity page."@en ;
                rdfs:label "AniDB link"@en .


###  http://mediagraph.link/anidb/ont/officialTitle
anidb:officialTitle rdf:type owl:DatatypeProperty ;
                    rdfs:domain anidb:CreativeWork ;
                    rdfs:range rdfs:Literal ;
                    rdfs:comment "An official title in a given language."@en ;
                    rdfs:label "Official title"@en .


###  http://mediagraph.link/anidb/ont/primaryTitle
anidb:primaryTitle rdf:type owl:DatatypeProperty ;
                   rdfs:domain anidb:CreativeWork ;
                   rdfs:range rdfs:Literal ;
                   rdfs:label "Primary title"@en .


###  http://mediagraph.link/anidb/ont/shortTitle
anidb:shortTitle rdf:type owl:DatatypeProperty ;
                 rdfs:domain anidb:CreativeWork ;
                 rdfs:range rdfs:Literal ;
                 anidb:comment "A shortened version of the title. [Source: https://wiki.anidb.net/Content:Titles#Shorttitle ]"@en ;
                 rdfs:label "Short title"@en .


###  http://mediagraph.link/anidb/ont/synonym
anidb:synonym rdf:type owl:DatatypeProperty ;
              rdfs:domain anidb:CreativeWork ;
              rdfs:range rdfs:Literal ;
              anidb:comment "For anything not official, which is widely used, or if it is sold/aired under more than one name, set them as synonyms/aliases. [Source: https://wiki.anidb.net/Content:Titles#Synonym.2FAlias ]"@en ;
              rdfs:label "Synonym"@en .


